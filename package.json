{
  "name": "react-app-template",
  "version": "0.0.0",
  "private": true,
  "description": "React App Template is a standard for building react project.",
  "keywords": [
    "react",
    "template",
    "frontend"
  ],
  "author": {
    "name": "Yernar.T",
    "email": "toktaryernar@gmail.com",
    "url": "https://github.com/YernarT"
  },
  "bugs": {
    "url": "https://github.com/YernarT/react-app-template/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/YernarT/react-app-template"
  },
  "license": "MIT",
  "dependencies": {
    "antd": "^4.16.9",
    "axios": "^0.21.1",
    "craco": "^0.0.3",
    "craco-antd": "^1.19.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "redux": "3.7.2",
    "redux-thunk": "^2.3.0",
    "shortid": "^2.2.16",
    "store": "^2.0.12"
  },
  "devDependencies": {
    "eslint": "^7.31.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^7.0.1",
    "lint-staged": "^11.1.1",
    "prettier": "^2.3.2",
    "raw-loader": "^4.0.2",
    "redux-devtools-extension": "^2.13.9"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/*.{js,jsx,mjs,ts,tsx}": [
      "node_modules/.bin/prettier --write",
      "node_modules/.bin/eslint --fix",
      "git add"
    ],
    "src/*.{css,scss,less,json,html,md,markdown}": [
      "node_modules/.bin/prettier --write",
      "git add"
    ]
  },
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "craco test",
    "eject": "react-scripts eject"
  },
  "cracoConfig": "src/config/craco.config.js",
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
